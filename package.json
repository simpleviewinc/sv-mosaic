	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./transforms": {
			"types": "./dist/types/transforms/index.d.ts",
			"import": "./dist/esm/transforms/index.js",
			"require": "./dist/cjs/transforms/index.js"
		},
		"./formActions": {
			"types": "./dist/types/components/Form/formActions.d.ts",
			"import": "./dist/esm/components/Form/formActions.js",
			"require": "./dist/cjs/components/Form/formActions.js"
		},
		"./components/*": {
			"types": "./dist/types/components/*/index.d.ts",
			"import": "./dist/esm/components/*/index.js",
			"require": "./dist/cjs/components/*/index.js"
		}
	},
	"scripts": {
		"test": "jest",
		"tsc": "tsc",
		"start": "yarn run storybook",
		"build": "yarn run build:esm && yarn run build:types && yarn run build:cjs",
		"build:esm": "rm -rf ./dist/esm && rm -rf ./dist/types && ttsc --project ./tsconfig.esm.json",
		"build:cjs": "rm -rf ./dist/cjs && ttsc --project ./tsconfig.cjs.json",
		"build:storybook": "build-storybook -c .storybook -o docs",
		"build:types": "./scripts/build-types",
		"ci:build": "./scripts/ci-build",
		"ci:storybook": "./scripts/ci-storybook",
		"docker": "./scripts/build && ./scripts/run || true",
		"git-deploy": "node ./scripts/git-deploy.js",
		"conditional-publish": "./scripts/conditional-publish",
		"publish:build": "node ./scripts/publish-build.js",
		"storybook": "start-storybook -p 10000",
		"style": "eslint . --fix",
		"sync-state": "./scripts/sync-state",
		"types": "tsc --noEmit"
	},
	"files": [
		"dist"
	],
	"packageManager": "yarn@3.1.1",
